{
  "id": {
    "type": "string",
    "minLength": 1,
    "maxLength": 256,
    "pattern": "^\\S+$"
  },
  "name": {
    "type": "string",
    "minLength": 1,
    "maxLength": 256,
    "pattern": "^\\S+.*\\S+$"
  },
  "schedule": {
    "type": "string",
    "pattern": "^R\\/(\\d{4})-(0?[1-9]|1[0-2])-(0?[1-9]|[1-2][0-9]|3[0-1])T([0-1]?\\d|2[0-3]):([0-5]?\\d):([0-5]?\\d)Z\\/P(?:(\\d+)Y(\\d+)M(\\d+)D|(\\d+)M(\\d+)D|(\\d+)Y(\\d+)D|(\\d+)Y(\\d+)M|(\\d+)Y|(\\d+)M|(\\d+)D)?T(?:(\\d+)H(\\d+)M(\\d+)S|(\\d+)M(\\d+)S|(\\d+)H(\\d+)S|(\\d+)H(\\d+)M|(\\d+)H|(\\d+)M|(\\d+)S)?\\/E(?:(\\d+)Y(\\d+)M(\\d+)D|(\\d+)M(\\d+)D|(\\d+)Y(\\d+)D|(\\d+)Y(\\d+)M|(\\d+)Y|(\\d+)M|(\\d+)D)?T(?:(\\d+)H(\\d+)M(\\d+)S|(\\d+)M(\\d+)S|(\\d+)H(\\d+)S|(\\d+)H(\\d+)M|(\\d+)H|(\\d+)M|(\\d+)S)?$"
  },
  "urn": {
    "type": "string",
    "minLength": 1,
    "pattern": "^(\\S+):\/\/(\\S+)$"
  },
  "payload": {
    "type": "object"
  }
}
